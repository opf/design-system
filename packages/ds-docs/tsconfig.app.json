/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "paths": [
      "@angular/*": [
        "./node_modules/@angular/*"
      ],
      "@angular-devkit/*": [
        "./node_modules/@angular-devkit/*"
      ]
    ],
    "outDir": "../../out-tsc/app",
    "types": []
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ],
  "angularCompilerOptions": {
    "preserveSymlinks": true
  }
}
